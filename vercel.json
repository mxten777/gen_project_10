{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "functions": {},
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/invitation",
      "destination": "/invitation.html"
    },
    {
      "source": "/checkin",
      "destination": "/checkin.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    }
  ]
}